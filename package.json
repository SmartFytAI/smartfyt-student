{
  "name": "smartfyt-student",
  "version": "1.0.0",
  "description": "SmartFyt Student Experience - Modern Next.js 15 app for student-athletes",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:local": "next dev --turbo",
    "dev:static": "FAST_REFRESH=false next dev",
    "dev:clean": "rm -rf .next && rm -rf node_modules/.cache && next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "eslint . --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
